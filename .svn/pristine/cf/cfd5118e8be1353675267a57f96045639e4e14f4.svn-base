<?xml version="1.0" encoding="utf-8"?>
<!-- 작성일 : 2015.10.22
     작성자 : IT표준화팀 황진광 -->
<xs:schema 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
  <xs:include schemaLocation="NMI_REQ_HEADER.xsd"/>

  <xs:element name="CRWDFSW01RQ0002M00">
    <!-- NMI 헤더 -->
    <xs:complexType>
      <!-- 크라우드펀딩 투자자 투자한도 요청 -->
      <xs:complexContent>
        <xs:extension base="NMI_REQ_HEADER">
          <xs:sequence>
            <xs:element name="RQ_ACOPNO" minOccurs="1">
              <xs:simpleType>
                <!-- 요청계좌개설자번호 -->
                <xs:restriction base="xs:integer">
                  <xs:totalDigits value="6"/>
                  <xs:whiteSpace value="collapse"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="RQ_ACOPSEQ" minOccurs="1">
              <xs:simpleType>
                <!-- 요청계좌개설일련번호 -->
                <xs:restriction base="xs:string">
                  <xs:maxLength value="4"/>
                  <xs:whiteSpace value="collapse"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="REQ_SEQ" minOccurs="1">
              <!-- 요청일련번호 -->
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:totalDigits value="10"/>
                  <xs:whiteSpace value="collapse"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            
            <xs:element name="REQ_TPCD" minOccurs="1">
              <!-- 요청구분코드 -->
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="2"/>
                  <xs:whiteSpace value="collapse"/>
                  <xs:enumeration value="01"/>
                  <xs:enumeration value="02"/>
                  <xs:enumeration value="09"/>
                  <xs:enumeration value="91"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 발행인사업자등록번호 -->
            <xs:element name="ISSUR_BUZNO" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="10"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 증권구분 -->
            <xs:element name="SECN_TPCD" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="2"/>
                  <xs:enumeration value="11"/>
                  <xs:enumeration value="21"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 종목코드 -->
            <xs:element name="ISIN" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="12"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 청약개시일 -->
            <xs:element name="SUBS_BEGIN_DT" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:totalDigits value="8"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 청약종료일-->
            <xs:element name="SUBS_EXPRY_DT" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:totalDigits value="8"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 투자자실명번호 -->
            <xs:element name="INVSTR_REALNMNO" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="20"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>


            <!-- 실명번호구분 -->
            <xs:element name="INVSTR_REALNMNO_TPCD" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="2"/>
                  <xs:enumeration value="11"/>
                  <xs:enumeration value="12"/>
                  <xs:enumeration value="21"/>
                  <xs:enumeration value="22"/>
                  <xs:enumeration value="23"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 계좌확인실명번호 -->
            <xs:element name="INVSTR_BUZNO" minOccurs="0">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="20"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            
            <!-- 투자자유형 -->
            <xs:element name="INVSTR_SORT_CD" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="2"/>
                  <xs:enumeration value="01"/>
                  <xs:enumeration value="02"/>
                  <xs:enumeration value="03"/>
                  <xs:enumeration value="04"/>
                  <xs:enumeration value="05"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 투자자성명 -->
            <xs:element name="INVSTR_NM" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="200"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 청약금액  -->
            <xs:element name="SUBS_AMT" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:totalDigits value="18"/>
                  <xs:fractionDigits value="2"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 청약수량  -->
            <xs:element name="SUBS_QTY" minOccurs="1">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:totalDigits value="16"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 위탁사번호  -->
            <xs:element name="SECUCO_ACOPNO" minOccurs="0">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:totalDigits value="6"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 위탁사계좌일련번호  -->
            <xs:element name="SECUCO_ACOPSEQ" minOccurs="0">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="4"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 위탁사계좌분류코드  -->
            <xs:element name="SECUCO_ACOACD" minOccurs="0">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="2"/>
                  <xs:enumeration value="01"/>
                  <xs:enumeration value="02"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <!-- 위탁계좌번호  -->
            <xs:element name="INVSTR_TRSAC_NO" minOccurs="0">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="30"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="VERSION" type="xs:string" fixed="1.5"/>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
</xs:schema>
